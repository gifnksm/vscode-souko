{
  "name": "souko",
  "displayName": "souko",
  "description": "Integrates souko into Visual Studio Code, a simple command line utility to organize clones of remote git repositories",
  "version": "0.1.0",
  "publisher": "gifnksm",
  "homepage": "https://github.com/gifnksm/vscode-souko",
  "bugs": {
    "url": "https://github.com/gifnksm/vscode-souko/issues"
  },
  "license": "MIT",
  "author": {
    "name": "gifnksm",
    "email": "makoto.nksm+github@gmail.com",
    "url": "https://github.com/gifnksm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gifnksm/vscode-souko"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "souko.open",
        "title": "souko: Open repository"
      },
      {
        "command": "souko.openInNewWindow",
        "title": "souko: Open repository in new window"
      }
    ],
    "configuration": {
      "title": "Souko",
      "properties": {
        "souko.command": {
          "type": "string",
          "default": "souko",
          "description": "The command to execute souko command line utilify",
          "ignoreSync": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/glob": "^9.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.25",
    "@types/vscode": "^1.106.1",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
